---
import Certificates from "./Certificates.astro";
import Contact from "./Contact.astro";
import Skills from "./Skills.astro";
import TechStack from "./TechStack.astro";
import Contributions from "./Contributions.astro";
---

<aside>
  <Contact />
  <Contributions />
  <Skills />
  <TechStack />
  <Certificates />
</aside>

<style>
  aside {
    display: flex;
    flex-direction: column;
    gap: 1.3em;
  }

  /* Print styles */
  @media only print {
    aside {
      gap: 0.3em;
    }
  }
</style>
